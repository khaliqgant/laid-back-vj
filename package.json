{
  "name": "laid-back-vj",
  "description": "Sit back and watch all of your favorite songs music videos",
  "homepage": "https://ladbackvj.com",
  "author": {
    "name": "Khaliq Gant",
    "email": "khaliqgant@gmail.com",
    "url": "https://github.com/khaliqgant"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khaliqgant/"
  },
  "engines": {
    "node": ""
  },
  "bin": {
    "": ""
  },
  "scripts": {
    "css:compile": "./node_modules/.bin/gulp sass",
    "css:compile:watch": "./node_modules/.bin/gulp",
    "fix:tslint": "./node_modules/.bin/tslint --project ./tsconfig.json --fix --force",
    "fix:prettier-eslint": "./node_modules/.bin/prettier-eslint **.ts --write",
    "fix:eslint": "./node_modules/.bin/eslint --fix --ext .ts .; exit 0",
    "fix": "./node_modules/.bin/run-p fix:tslint fix:prettier-eslint",
    "fix:watch": "./node_modules/.bin/esw --fix --ext .ts . --watch",
    "build:tsc": "./node_modules/.bin/tsc",
    "build": "npm run fix && npm run build:tsc && npm run build:fe",
    "build:watch:ts": "./node_modules/.bin/tsc -w",
    "build:watch:fe": "./node_modules/.bin/webpack --progress ./webpack.config.js -w",
    "build:watch": "./node_modules/.bin/run-p build:watch:ts build:watch:fe",
    "build:fe": "./node_modules/.bin/webpack --progress ./webpack.config.js",
    "clean": "./node_modules/.bin/rimraf **.js **.js.map",
    "watch": "npm run clean && ./node_modules/.bin/run-p fix:watch build:watch",
    "precommit": "npm run fix",
    "serve": "./node_modules/.bin/nodemon app.js",
    "dev": "./node_modules/.bin/run-p css:compile:watch fix:watch build:watch serve"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "browser-request": "^0.3.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "handlebars": "^4.0.11",
    "lastfmapi": "^0.1.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "q": "^1.5.0",
    "request": "^2.83.0",
    "rollbar": "^2.2.10",
    "serve-favicon": "^2.4.5",
    "youtube-node": "^1.3.2"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/express": "^4.0.37",
    "@types/q": "^1.0.5",
    "@types/request": "^2.0.5",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.9.0",
    "gulp-develop-server": "^0.5.2",
    "gulp-mocha": "^4.3.1",
    "gulp-sass": "^3.1.0",
    "gulp-typescript": "^3.2.2",
    "gulp-webpack": "^1.5.0",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.0",
    "rimraf": "^2.6.2",
    "should": "^13.1.2",
    "ts-loader": "^2.3.7",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "typescript-eslint-parser": "^11.0.0",
    "typings": "^2.1.1",
    "webpack": "^3.7.1"
  },
  "keywords": [
    ""
  ]
}
